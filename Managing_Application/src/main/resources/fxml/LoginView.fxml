<?xml version="1.0" encoding="UTF-8"?>

<!--suppress JavaFxUnresolvedFxIdReference -->

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Font?>

<Pane fx:id="loginPane" prefHeight="547.0" prefWidth="465.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.controllers.LoginViewController">
    <children>
        <Label layoutX="95.0" layoutY="84.0" text="Welcome!" textFill="#464646">
            <font>
                <Font name="Lato Light Italic" size="64.0" />
            </font>
        </Label>
        <TextField text="administrator" fx:id="username" layoutX="114.0" layoutY="263.0" prefHeight="42.0" prefWidth="237.0" promptText="Login" />
        <PasswordField text="administrator" fx:id="password" layoutX="114.0" layoutY="314.0" onAction="#onEnter" prefHeight="42.0" prefWidth="237.0" promptText="Password" />
      <!--suppress JavaFxUnresolvedFxIdReference -->
        <Button fx:id="loginButton" layoutX="114.0" layoutY="381.0" mnemonicParsing="false" onAction="#handleLogIn" prefHeight="29.0" prefWidth="237.0" text="Log in" />
    </children>
    <stylesheets>
        <URL value="@../css/style.css"/>
    </stylesheets>
</Pane>
